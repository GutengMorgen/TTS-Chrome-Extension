{
    "manifest_version" : 3,
    "name" : "TTS extension",
    "version" : "1.0",
    "description" : "este es una extension",
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "css": ["styles.css"],
          "js": ["content.js"]
        }
    ],
    "icons" : {
        "128" : "logo.png"
    },
    "action" : {
        "default_popup" : "index.html"
    },
    "permissions": [ "activeTab"]
    
}